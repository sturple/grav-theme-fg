{% embed 'partials/base.html.twig' %}

{% block blog_posts %}
<div class="row">
    <div class="col-md-9">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-9">{{ block('posts') }}   </div>
        </div>
    </div>
    <div class="col-md-3">        
    {{ block('sidebar') }}
    </div>   
</div>
{% endblock %}


{% block content %}
    {% use 'blog/blog_theme.html.twig' %}
   

       
    {% set collection = page.collection() %}
    
    <div class="container">
        {{ block('breadcrumbs') }}
        {{ block('blog_content') }}
        {{ block('blog_posts') }}
        {{ block('pagination') }}
    </div>
{% endblock %}
  

	

{% endembed %}


