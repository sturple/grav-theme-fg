{# Check if youtube or feature image #}

{% if page.header.slideshow == 'youtube' %}
    {{turpyoutube }}
{%elseif page.header.slideshow == 'image' %}
    {% if page.header.feature.image %}
        {% set featureImage = page.media[page.header.feature.image] %}
    {% else %}
        {% set featureImage = page.media.images|first %}
    {% endif%}

    <img class="img-responsive" style="width: 100%"
         data-image-lg="{{featureImage.cropResize(1920,1920).url()}}"
         data-image-md="{{featureImage.cropResize(1200,1200).url()}}"
         data-image-sm="{{featureImage.cropResize(768,768).url()}}"
         alt="{{page.title}}"/>
   
{% endif%}	
